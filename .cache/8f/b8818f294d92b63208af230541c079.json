ages/property-list/property-list.mappers.js","original":{"line":42,"column":34}},{"generated":{"line":49,"column":0},"source":"pages/property-list/property-list.mappers.js","original":{"line":43,"column":1}},{"generated":{"line":49,"column":1},"source":"pages/property-list/property-list.mappers.js","original":{"line":43,"column":2}},{"generated":{"line":50,"column":0},"source":"pages/property-list/property-list.mappers.js","original":{"line":43,"column":3}}],"sources":{"pages/property-list/property-list.mappers.js":"export const mapPropertyListApiToVm = (propertyList) =>\n  Array.isArray(propertyList)\n    ? propertyList.map((property) => mapPropertyApiToVm(property))\n    : [];\n\n    \nconst mapPropertyApiToVm = (property) => ({\n  id: property.id,\n  title: property.title,\n  rooms: `${property.rooms} ${getRoomWord(property.rooms)}`,\n  squareMeter: `${property.squareMeter}m2`,\n  notes: `${property.notes.substring(0, 240)}...`, //cortar en 240 caracteres\n  price: `${property.price.toLocaleString()} ‚Ç¨`,\n  image: Array.isArray(property.images) ? property.images[0] : \"\",\n});\n\n\nconst getRoomWord = (rooms) => (rooms > 1 ? \"habitaciones\" : \"habitaci√≥n\");\n\n\n\n export const mapFilterToQueryParams = (filter) => {\n   let queryParams = \"\";\n   if (filter.saleTypeId) {\n     queryParams = `${queryParams}saleTypeIds_like=${filter.saleTypeId}&`;\n   }\n   if (filter.provinceId) {\n     queryParams = `${queryParams}provinceId=${filter.provinceId}&`;\n   }\n   if (filter.minRooms) {\n     queryParams = `${queryParams}rooms_gte=${filter.minRooms}&`;\n   }\n   if (filter.minBathrooms) {\n     queryParams = `${queryParams}bathrooms_gte=${filter.minBathrooms}&`;\n   }\n   if (filter.minPrice) {\n     queryParams = `${queryParams}price_gte=${filter.minPrice}&`;\n   }\n   if (filter.maxPrice) {\n     queryParams = `${queryParams}price_lte=${filter.maxPrice}&`;\n   }\n   return queryParams.slice(0, -1);\n };"},"lineCount":null}},"error":null,"hash":"d694c907c4ab35